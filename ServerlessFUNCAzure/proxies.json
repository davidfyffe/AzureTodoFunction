{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxyHomePage": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/"
      },
      "backendUri": "https://dvtestsite.z5.web.core.windows.net/index.html"
    },
    "proxyContent": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/{*restOfPath}"
      },
      "backendUri": "https://dvtestsite.z5.web.core.windows.net/{restOfPath}"
    }
  }
}